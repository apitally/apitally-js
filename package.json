{
  "name": "apitally",
  "version": "1.0.0",
  "description": "API monitoring for REST APIs built with Express, Fastify, Koa, and NestJS.",
  "author": "Apitally <hello@apitally.io>",
  "license": "MIT",
  "keywords": [
    "analytics",
    "api",
    "apitally",
    "express",
    "fastify",
    "koa",
    "logging",
    "middleware",
    "monitor",
    "monitoring",
    "nestjs",
    "node",
    "nodejs",
    "plugin",
    "rest",
    "restful"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/apitally/nodejs-client.git"
  },
  "bugs": {
    "url": "https://github.com/apitally/nodejs-client/issues"
  },
  "homepage": "https://apitally.io",
  "type": "module",
  "scripts": {
    "build:tsc": "tsc",
    "build:cjs": "BABEL_ENV=cjs babel lib/tsc --out-dir lib/cjs --out-file-extension .cjs",
    "build:esm": "BABEL_ENV=esm babel lib/tsc --out-dir lib/esm",
    "build": "rm -rf lib && npm run build:tsc && npm run build:cjs && npm run build:esm",
    "format": "prettier --write .",
    "check": "tsc --noEmit && eslint src tests && prettier --check .",
    "test": "vitest"
  },
  "files": [
    "lib/cjs/",
    "lib/esm/",
    "lib/types/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    "./common": {
      "import": "./lib/esm/common/index.js",
      "require": "./lib/cjs/common/index.cjs",
      "types": "./lib/types/common/index.d.ts"
    },
    "./express": {
      "import": "./lib/esm/express/index.js",
      "require": "./lib/cjs/express/index.cjs",
      "types": "./lib/types/express/index.d.ts"
    },
    "./fastify": {
      "import": "./lib/esm/fastify/index.js",
      "require": "./lib/cjs/fastify/index.cjs",
      "types": "./lib/types/fastify/index.d.ts"
    },
    "./koa": {
      "import": "./lib/esm/koa/index.js",
      "require": "./lib/cjs/koa/index.cjs",
      "types": "./lib/types/koa/index.d.ts"
    },
    "./nestjs": {
      "import": "./lib/esm/nestjs/index.js",
      "require": "./lib/cjs/nestjs/index.cjs",
      "types": "./lib/types/nestjs/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"
    },
    "env": {
      "node": true
    },
    "root": true
  },
  "prettier": {
    "printWidth": 80
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "version": "2023-05"
        }
      ],
      "@babel/plugin-transform-runtime"
    ],
    "env": {
      "cjs": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": "commonjs"
            }
          ]
        ],
        "plugins": [
          "babel-plugin-transform-import-meta",
          [
            "transform-require-extensions",
            {
              "extensions": {
                ".js": ".cjs"
              }
            }
          ]
        ]
      },
      "esm": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ]
        ]
      }
    },
    "sourceMaps": true
  },
  "dependencies": {
    "@babel/runtime": "^7.23.8",
    "axios": "^1",
    "axios-retry": "^4",
    "winston": "^3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8 || ^9 || ^10",
    "@nestjs/core": "^8 || ^9 || ^10",
    "@nestjs/platform-express": "^8 || ^9 || ^10",
    "express": "^4",
    "fastify": "^3 || ^4",
    "fastify-plugin": "^3 || ^4",
    "koa": "^2"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    },
    "@nestjs/platform-express": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "fastify-plugin": {
      "optional": true
    },
    "koa": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@koa/router": "^12.0.1",
    "@nestjs/common": "^10.2.9",
    "@nestjs/core": "^10.2.9",
    "@nestjs/platform-express": "^10.2.9",
    "@nestjs/testing": "^10.2.9",
    "@swc/core": "^1.3.104",
    "@types/express": "^4.17.21",
    "@types/koa": "^2.13.11",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-route": "^3.2.8",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitest/coverage-v8": "^1.5.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "babel-plugin-transform-require-extensions": "^2.0.1",
    "celebrate": "^15.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.54.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "install": "^0.13.0",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-route": "^3.2.0",
    "nock": "^13.3.8",
    "npm": "^10.2.4",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.5.0"
  }
}
