    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@sentry/node": "^9.0.0",
    "@swc/core": "^1.3.104",
    "@types/async-lock": "^1.4.2",
    "@types/express": "^5.0.3",
